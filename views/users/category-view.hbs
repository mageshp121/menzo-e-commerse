                    <div class="product__item">
                        <div class="product__item__pic set-bg">
                            <a href="/singleproduct/{{this._id}}"><img class="product__item__pic set-bg"  src="/product-images/{{this._id}}.jpg"></a>
                            <span class="label">New</span>
                            <ul class="product__hover">
                                <li><a href="/addTo-wishlist/{{this._id}}"><img src="../img/icon/heart.png" alt=""></a></li>
                                <li><a href="#"><img src="../img/icon/compare.png" alt=""> <span>Compare</span></a></li>
                                <li><a href="#"><img src="../img/icon/search.png" alt=""></a></li>
                            </ul>
                        </div>
                        <div class="product__item__text">
                            <h6>{{this.productname}}</h6>
                            <a onclick="addToCart('{{this._id}}')" class="add-cart">+ Add To Cart</a>
                            <div class="rating">
                                <i class="fa fa-star-o"></i>
                                <i class="fa fa-star-o"></i>
                                <i class="fa fa-star-o"></i>
                                <i class="fa fa-star-o"></i>
                                <i class="fa fa-star-o"></i>
                            </div>
                            <h5>â‚¹{{this.price}}</h5>
                            <div class="product__color__select">
                                <label for="pc-1">
                                    <input type="radio" id="pc-1">
                                </label>
                                <label class="active black" for="pc-2">
                                    <input type="radio" id="pc-2">
                                </label>
                                <label class="grey" for="pc-3">
                                    <input type="radio" id="pc-3">
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
         </div>
        </section>